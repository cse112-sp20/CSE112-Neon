{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
	  "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
	  "import/no-unresolved": "off",
	  "prefer-rest-params": "off",
	  "no-console": 0
    }
}
